{
  "name": "intecu-network-test",
  "version": "1.0.0",
  "description": "OAU Network support tool",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/oaulogosmaller.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/oaulogosmaller.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"INTECU Network Test\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/oaulogosmaller.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adisen/intecu-app.git"
  },
  "author": "IT STUDENTS (2018/2019)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adisen/intecu-app/issues"
  },
  "homepage": "https://github.com/adisen/intecu-app#readme",
  "dependencies": {
    "systeminformation": "^5.3.1"
  },
  "devDependencies": {
    "electron": "^11.5.0",
    "electron-packager": "^14.2.1",
    "nodemailer": "^6.6.1",
    "pdfkit": "^0.11.0",
    "request": "^2.88.2",
    "xmlhttprequest": "^1.8.0"
  }
}
